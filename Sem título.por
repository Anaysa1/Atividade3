programa {
  funcao inicio() {
  
    real n1, n2, rs
    escreva("Digite dois números para realizar os calculos ","\n")
    escreva("Digite o primeiro número:")
    leia(n1)
    escreva("Digite o segundo número:")
    leia(n2)
    rs = n1 / n2
    escreva("O resultado da sua divisão é: ", rs, "\n")
    rs = n1 + n2
    escreva("O resultado da soma é: " + rs)
    

  }
}

